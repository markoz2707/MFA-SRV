      <presentation id="POL_EndpointAgent_PipeTimeoutMs">
        <decimalTextBox refId="DEC_EndpointAgent_PipeTimeoutMs" defaultValue="3000" spinStep="100">Limit czasu potoku (ms):</decimalTextBox>
