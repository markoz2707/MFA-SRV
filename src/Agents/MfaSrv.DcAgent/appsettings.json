{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    },
    "EventLog": {
      "SourceName": "MfaSrv DC Agent",
      "LogName": "Application",
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Grpc": {
        "Url": "http://0.0.0.0:5090",
        "Protocols": "Http2"
      }
    }
  },
  "DcAgent": {
    "CentralServerUrl": "https://mfasrv-server:5081",
    "AgentId": "",
    "PipeName": "MfaSrvDcAgent",
    "PipeTimeoutMs": 3000,
    "HeartbeatIntervalSeconds": 30,
    "PolicySyncIntervalSeconds": 60,
    "SessionTtlMinutes": 480,
    "CertificatePath": "",
    "CertificatePassword": "",
    "GossipPeers": [],
    "FailoverMode": "FailOpen",
    "CacheDbPath": "dcagent_cache.db"
  }
}
